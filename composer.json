{
    "name": "briteweb/cli",
    "version": "0.0.3",
    "description": "The Laravel Zero Framework.",
    "keywords": ["wordpress", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/briteweb/bw-cli",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/briteweb/bw-cli/issues",
        "source": "https://github.com/briteweb/bw-cli"
    },
    "authors": [
        {
            "name": "Lindsay Stewart",
            "email": "lindsay.stewart@briteweb.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "wp-cli/wp-cli": "^2.5",
        "wp-cli/wp-cli-bundle": "^2.5"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/bw"]
}
